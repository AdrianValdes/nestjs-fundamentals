{
    "linter": {
        "rules": {
            "suspicious": {
                "noPrototypeBuiltins": "off"
            },
            "style": {
                "useImportType": "off"
            },
            "correctness": {
                "noUnusedImports": "error"
            }
        }
    },
    "javascript": {
        "parser": {
            "unsafeParameterDecoratorsEnabled": true
        }
    }
}